version 16
__lua__
function _init()
	t=0	-- general ticker
	game_over=false
	make_player(24,60)
	make_enemies(1)
	bullets={}
	fx1_init()
	smoke={}
end

function _update()
	t+=1	-- increase ticker
	foreach(smoke,move_smoke)
	
 if (not game_over) then
 	move_player()
		check_collision()
		make_smoke(player.x,player.y,.1)
	else
		if btnp(❎) then _init() end
	end
	
	update_fx(blow,fx1)
	update_bullets()
end

function _draw()
	cls(14)	-- bg
	rectfill(0,100,127,127,12) -- ocean

	print("x:"..player.x,0,0,15)
	print("y:"..player.y,0,8,15)
	
	if (not game_over) then
		draw_player()
		draw_enemies()
	else
		print("press ❎ to restart",30,60,15)
	end	

	fillp(0b1010010110100101.1)
	foreach(smoke,draw_smoke)
	fillp()

	draw_fx(blow,fx1)
	draw_bullets()
end
-->8
function make_plane(x,y,sp)
	local p={}
	p.x=x
	p.y=y
	p.sp=sp
	p.ox=-15
	p.oy=-10
	return p
end

function draw_plane(p,flip_h)
	spr(p.sp,p.x+p.ox,p.y+p.oy,2,2,flip_h,false)
end

function blow_plane(p)
	for i=1,fx1.burst do
		add_p(p.x,p.y,blow,fx1)
	end
end
-->8
function make_player(x,y)
	player = make_plane(x,y,1)
end

function move_player()
	if btn(0) then player.x-=1 end
	if btn(1) then player.x+=1 end
	if btn(2) then player.y-=1 end
	if btn(3) then player.y+=1 end
	if btnp(4) then fire(player) end
end

function draw_player()
	draw_plane(player,false)
	spr(3+t%2,player.x+player.ox,player.y+player.oy)
	spr(19+t%2,player.x+16+player.ox,player.y+8+player.oy)
end

function check_collision()
	for e in all(enemies) do
			if player.x+1>=e.x-1 and
			player.x-12<=e.x+12 and
			player.y+3>=e.y-5 and
			player.y-5<=e.y+3 then
				game_over=true 
				blow_plane(player)
				blow_plane(e)
			end
		end
end

function fire(p)
	local b={
		x=p.x,
		y=p.y,
		dx=3,
		sp=16
	}
	add(bullets,b)
end

function update_bullets()
	for b in all(bullets) do
		b.x+=b.dx
		if b.x<=0 or b.x>=128 then
			del(bullets,b)
		end
	end
end

function draw_bullets()
	for b in all(bullets) do
		spr(b.sp,b.x,b.y)
	end
end
-->8
function make_enemies(qty)
	enemies={}
	for i=1,qty do
		local e=make_plane(100,rnd(80)+16,32)
		e.ox=0 -- correction for flip
		add(enemies,e)
	end
end

function draw_enemies()
	for e in all(enemies) do
		draw_plane(e,true)
		spr(19+t%2,e.x-1,e.y-2)
	end
end
-->8
function fx1_init()
	fx1={						-- settings for fx1 (blow)
		g=0.1,						-- particle grav
		max_vel=3,		--	initial vel
		min_time=2,	-- time between particles
		max_time=5,
		min_life=30,-- particle lifetime
		max_life=50,
		t=0,								-- particle ticker
		cols={2,2,2,8,8,9,9,10}, -- colors
		burst=100
	}
	blow={}			-- empty table
end

function rndb(low,high)
	return flr(rnd(high-low+1)+low)
end

function add_p(x,y,fx_holder,fx)
	local p={}
	p.x,p.y=x,y
	p.dx=rnd(fx.max_vel)-fx.max_vel/2
	p.dy=rnd(fx.max_vel)*-1
	p.life_start=rndb(fx.min_life,fx.max_life)
	p.life=p.life_start
	add(fx_holder,p)
end

function update_fx(fx_holder,fx)
	for p in all(fx_holder) do
		if p.life<=0 then
			del(fx_holder,p)
		else
			p.dy+=fx.g
			p.x+=p.dx
			p.y+=p.dy
			p.life-=1
		end
	end	
end

function draw_fx(fx_holder,fx)
	for p in all(fx_holder) do
		local pcol=flr(p.life/p.life_start*#fx.cols+1)
		pset(p.x,p.y,fx.cols[pcol])
	end
end
-->8
function new_smoke(x,y,init_size)
	local s={}
	s.x=x
	s.y=y
	s.col=5+rnd(2)
	s.size=init_size
	s.max_size=init_size+rnd(2)+init_size
	s.t=0
	s.max_life=10+rnd(10)
	s.dx=rnd(.4)-.2
	s.dy=-rnd(.5)
	s.ddy=-.02
	return s
end

function make_smoke(x,y,size)
	smoke_puff=new_smoke(x,y,size)
	add(smoke,smoke_puff)
end

function move_smoke(s)
	if s.t>s.max_life then
		del(smoke,s)
		return
	elseif s.t>s.max_life-15
	and s.size<s.max_size then
		s.size+=.5
	end
	
	s.x+=s.dx
	s.y+=s.dy
	s.dy+=s.ddy
	s.t+=1
end

function draw_smoke(s)
	circfill(s.x,s.y,s.size,s.col)
end
